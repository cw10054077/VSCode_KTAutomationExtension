{
  "name": "vscode-selfhost-test-provider",
  "displayName": "VS Code Selfhost Test Provider",
  "description": "Test provider for the VS Code project",
  "enabledApiProposals": [
    "testObserver"
  ],
  "icon": "icon.png",
  "version": "0.3.20",
  "publisher": "ms-vscode",
  "engines": {
    "vscode": "^1.59.0-20210722"
  },
  "categories": [
    "Other"
  ],
  "capabilities": {
    "codeLensProvider": {
        "resolveProvider": "true"
    }
  },
  "activationEvents": [
    "workspaceContains:src/vs/loader.js",
    "*"
  ],
  "contributes": {
    "commands": [
        {
            "command": "extension.testSnippet",
            "title": "test snippet"
        },
        {
            "command": "testnotes.createNote",
            "title": "Create Note"
        },
        {
            "command": "testnotes.saveNote",
            "title": "Save Note"
        },
        {
            "command": "testnotes.dispose",
            "title": "Remove All Notes"
        },
        {
            "command": "calicoColors.addColor",
            "category": "Calico Colors",
            "title": "Add Color"
        },
        {
            "command": "calicoColors.clearColors",
            "category": "Calico Colors",
            "title": "Clear Colors",
            "icon": "$(clear-all)"
        }
    ],
    "menus": {
        "view/title": [
            {
                "command": "calicoColors.clearColors",
                "group": "navigation",
                "when": "view == calicoColors.colorsView"
            }
        ],
        "comments/commentThread/context": [
            {
                "command": "testnotes.createNote",
                "group": "inline",
                "when": "commentController == test-commenter && commentThreadIsEmpty"
            }
        ],
        "comments/comment/context": [
            {
                "command": "testnotes.saveNote",
                "group": "inline@2",
                "when": "commentController == test-commenter"
            }
        ]
    },
    "views": {
      "explorer": [
        {
            "type": "webview",
            "id": "calicoColors.colorsView",
            "name": "KT Framework Config"
        }
      ]
    }
  },
  "workspaceTrust": {
    "request": "onDemand",
    "description": "Trust is required to execute tests in the workspace."
  },
  "main": "./dist/extension.js",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2,
    "arrowParens": "avoid"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-selfhost-test-provider.git"
  },
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/vscode-selfhost-test-provider/issues"
  },
  "homepage": "https://github.com/microsoft/vscode-selfhost-test-provider#readme",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "fmt": "prettier --write \"src/**/*.ts\"&& npm run test -- --fix",
    "test": "tsc --noEmit && eslint src --ext ts",
    "postinstall": "vscode-dts main && vscode-dts dev"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@types/split2": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-plugin-header": "^3.1.1",
    "prettier": "^2.8.1",
    "ts-loader": "^9.4.2",
    "vscode-debugprotocol": "^1.49.0",
    "vscode-dts": "^0.3.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@azure/keyvault-secrets": "^4.7.0",
    "@jridgewell/trace-mapping": "^0.3.17",
    "@qoretechnologies/python-parser": "^0.4.10",
    "@types/vscode": "^1.84.0",
    "@vscode/python-extension": "^1.0.5",
    "ansi-styles": "^6.2.1",
    "js-base64": "^3.7.3",
    "node-fetch": "^3.3.2",
    "split2": "^4.1.0",
    "typescript": "^4.9.3"
  },
  "extensionDependencies": [
    "ms-python.python"
  ]
}
